{
  "name": "qr-code-generator-mcp",
  "version": "1.0.0",
  "description": "OpenAI Apps SDK - QR Code Generator with multiple input types",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "start:http": "MCP_TRANSPORT=http PORT=3001 node dist/server/index.js",
    "dev": "tsx watch server/index.ts",
    "dev:http": "MCP_TRANSPORT=http PORT=3001 tsx watch server/index.ts",
    "test": "echo \"Test by running the server and connecting via ChatGPT Developer Mode\""
  },
  "keywords": [
    "openai",
    "apps-sdk",
    "mcp",
    "qr-code"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "express": "^5.1.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
