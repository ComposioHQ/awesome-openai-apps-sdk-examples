{
  "name": "url-shortener-mcp",
  "version": "1.0.0",
  "description": "OpenAI Apps SDK - URL Shortener with analytics and link management",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "start:http": "MCP_TRANSPORT=http PORT=3000 node dist/server/index.js",
    "dev": "tsx watch server/index.ts",
    "dev:http": "MCP_TRANSPORT=http PORT=3000 tsx watch server/index.ts",
    "clean": "rm -rf dist data/links.json",
    "test": "echo \"Test by running the server and connecting via ChatGPT Developer Mode\""
  },
  "keywords": [
    "openai",
    "apps-sdk",
    "mcp",
    "url-shortener"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "express": "^4.18.2",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
